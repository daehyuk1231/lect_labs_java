package app.labs.ex02.dp02;

public abstract class RunningRace {
	private void ready() {
		System.out.println("준비");
	}

	protected abstract void run();

	private void finish() {
		System.out.println("결승선");
	}

	// template design dp
	// 메서드 실행 순서를 지
	public final void race() {
		ready();
		run();
		finish();
	}
}
